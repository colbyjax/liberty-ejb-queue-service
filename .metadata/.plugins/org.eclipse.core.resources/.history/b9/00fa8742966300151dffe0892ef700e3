package org.bitsteam.helloLiberty;
import javax.ws.rs.GET;
import javax.ws.rs.Path;

@Path("/Greeting")
// Type in browser: http://localhost:9080/HelloLibertyWeb/hello/Greeting
public class GreetingResource {
	@GET
	public String getMessage(){
		System.out.println("JAXRS - Greeting Service called...");
		return "Hello from your friendly Liberty RS Web Service";
	}
}
